<div id="<%= dom_id user %>">
  <p>
    <strong>User name:</strong>
    <%= user.user_name %>
  </p>

  <p>
    <strong>User password:</strong>
    <%= user.user_password %>
  </p>

  <p>
    <strong>User email:</strong>
    <%= user.user_email %>
  </p>

  <p>
    <strong>User firstname:</strong>
    <%= user.user_firstname %>
  </p>

  <p>
    <strong>User lastname:</strong>
    <%= user.user_lastname %>
  </p>


</div>
